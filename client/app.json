{
  "expo": {
    "name": "Abml expenses",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://advancedbml.engineering/api/uploads/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "https://advancedbml.engineering/api/uploads/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "adaptiveIcon": {
        "foregroundImage": "https://advancedbml.engineering/api/uploads/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "bundleIdentifier": "com.abml.expense",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload receipts."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://advancedbml.engineering/api/uploads/logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.naveed_arshad.myapp"
    },
    "web": {
      "favicon": "https://advancedbml.engineering/api/uploads/logo.png"
    },
    "extra": {
      "eas": {
        "projectId": "419cc52e-6956-4f48-bbc4-4be906554095"
      }
    },
    "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "We need access to your photo library to upload receipts.",
            "cameraPermission": "We need access to your camera to take receipt photos."
          }
        ]
      ]
  }
}
